{
    "name": "creativesofttechsolutions/laravelhooks",
    "description": "Wordpress Like Hooks System Package For Laravel",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "CreativeSoftTechSolutions\\LaravelHooks\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CreativeSoftTechSolutions\\LaravelHooks\\HooksServiceProvider"
            ],
            "aliases": {
                "Hooks": "CreativeSoftTechSolutions\\LaravelHooks\\Facades\\Hooks"
            }
        }
    },
    "minimum-stability": "stable"
}
